{
    "name": "webit/doctrine-json-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name": "Daniel Bojdo",
        "email": "daniel@bojdo.eu"
    }],
    "require": {
        "symfony/symfony": "^2.5|^3.0|^4.0",
        "doctrine/doctrine-bundle": "~1.2"
    },
    "require-dev": {
        "webit/doctrine-jms-json": "^1.0.0|^2.0.0",
        "jms/serializer-bundle": "^1.0",
        "phpunit/phpunit": "^4.8"
    },
    "suggest": {
        "webit/doctrine-jms-json": "To use JMS JSON type",
        "jms/serializer-bundle": "For easy bootstrapping of JMS Serializer (along with JMS JSON)"
    },
    "autoload": {
        "psr-4": {
            "Webit\\DoctrineJsonBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
